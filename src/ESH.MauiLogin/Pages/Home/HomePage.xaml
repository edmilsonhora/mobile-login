<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ESH.MauiLogin.Pages.Home.HomePage"
             Title="HomePage">
    <VerticalStackLayout HorizontalOptions="Center" Spacing="25" Padding="30">
        <Label 
            Text="Welcome to .NET MAUI!"
            VerticalOptions="Center" 
            HorizontalOptions="Center" />
        <Button x:Name="btnGoUsuarios" Text="Go Usuarios" Clicked="btnGoUsuarios_Clicked"></Button>
        <Button x:Name="btnDeslogar" Text="Fechar App" BackgroundColor="Red" Clicked="btnDeslogar_Clicked"></Button>

    </VerticalStackLayout>

</ContentPage>